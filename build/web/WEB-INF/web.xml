<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletMult</servlet-name>
        <servlet-class>aula.ServletMult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMult10</servlet-name>
        <servlet-class>aula.ServletMult10</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Mult100</servlet-name>
        <servlet-class>aula.Mult100</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMult100</servlet-name>
        <servlet-class>aula.ServletMult100</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletMult</servlet-name>
        <url-pattern>/ServletMult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMult10</servlet-name>
        <url-pattern>/ServletMult10</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mult100</servlet-name>
        <url-pattern>/Mult100</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMult100</servlet-name>
        <url-pattern>/ServletMult100</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
